{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node" },
    { "src": ".next/**/*", "use": "@vercel/next" },
    { "src": "public/**/*", "use": "copy" },
    { "use": "@vercel/static", "config": { "cleanUrls": true } },
    { "use": "@vercel/static-build", "config": { "command": "custom-build-command" } }
  ]
}